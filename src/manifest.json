{
  "manifest_version": 2,
  "name": "Form Unlocker",
  "description": "Enable all field from any form",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {"16": "img/icon16.png"},
  "browser_action": {
	"default_icon": "img/icon16.png"
  }  
}